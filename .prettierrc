{
     "editor.defaultFormatter": "esbenp.prettier-vscode",
     "editor.formatOnSave": true,

     "editor.rulers": [100],

     "printWidth": 100,
     "arrowParens": "always",
     "bracketSpacing": true,
     "htmlWhitespaceSensitivity": "css",
     "insertPragma": false,
     "jsxBracketSameLine": false,
     "jsxSingleQuote": false,
     "proseWrap": "preserve",
     "quoteProps": "as-needed",
     "requirePragma": false,
     "semi": true,
     "singleQuote": true,
     "trailingComma": "es5",
     "useTabs": false,
     "tabWidth": 5,
     "vueIndentScriptAndStyle": false
}
